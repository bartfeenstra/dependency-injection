{
  "name": "bartfeenstra/dependency-retriever",
  "license": "MIT",
  "require": {
    "php": "~5.6 || ~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "squizlabs/php_codesniffer": "~2.5"
  },
  "suggest": {
    "bartfeenstra/dependency-retriever-symfony-bridge": "Retrieves dependencies from Symfony's service container.",
    "drupal/retriever": "Retrieves dependencies from Drupal."
  },
  "autoload": {
    "psr-4": {
      "BartFeenstra\\DependencyRetriever\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BartFeenstra\\DependencyRetriever\\Fixtures\\": "fixtures",
      "BartFeenstra\\DependencyRetriever\\Tests\\": "tests"
    }
  }
}
